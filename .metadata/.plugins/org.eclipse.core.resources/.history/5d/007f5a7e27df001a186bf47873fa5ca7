public class Tree {
    int val;
    Tree l;
    Tree r;
    Tree() {}
    Tree(int val) { this.val = val; }
    Tree(int val, Tree left, Tree right) {
        this.val = val;
        this.l = l;
        this.r = r;
    }
	@Override
	public String toString() {
		return "TreeNode [val=" + val + "]";
	}
}